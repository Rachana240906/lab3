#include<stdio.h>
int main()
{
	int i=-1,j=0,esum=0,osum=0,num;
	printf("enter any number");
	scanf("%d",&num);
	while(i<num&&j<num)
	{
		i=i+2;
		osum=osum+i;
		j=j+2;
		esum=esum+j;
	}
	printf("\nthe sum of all odd numbers till %d is %d",num,osum);
	printf("\nthe sum of all even numbers till %d is %d",num,esum);
}
